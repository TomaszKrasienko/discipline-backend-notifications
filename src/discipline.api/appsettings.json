{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SignalR": {
    "System": {
      "Route": "/discipline-notifications-hub",
      "Method": "user-notifications"
    }
  },
  "Auth": {
    "PublicCertPath": "certs/public.pem",
    "Issuer": "discipline-web-app",
    "Audience": "discipline-web-app"
  },
  "Mongo": {
    "ConnectionString":"mongodb://localhost:6011",
    "Database": "discipline-notifications"
  },
  "Redis": {
    "ConnectionString": "localhost:6012"
  },
  "DefinitionsInitialization":{
    "Enabled": true
  } 
}
